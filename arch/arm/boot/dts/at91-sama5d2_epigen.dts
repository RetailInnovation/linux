/dts-v1/;
#include "sama5d2.dtsi"
#include "sama5d2-pinfunc.h"
#include <dt-bindings/mfd/atmel-flexcom.h>
#include <dt-bindings/gpio/gpio.h>

/ {
	model = "Retail Innovation Epigen";
	compatible = "retailinnovation,epigen", "atmel,sama5d2", "atmel,sama5";

	aliases {
		serial0 = &uart4;
		serial1 = &uart1;
		serial2 = &uart0;
		serial3 = &uart2;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};

	memory {
		reg = <0x20000000 0x10000000>;
	};

	clocks {
		slow_xtal {
			clock-frequency = <32768>;
		};

		main_xtal {
			clock-frequency = <12000000>;
		};
	};

	ahb {
		usb0: gadget@300000 {
			atmel,vbus-gpio = <&pioA PIN_PA31 GPIO_ACTIVE_HIGH>;
			pinctrl-names = "default";
			pinctrl-0 = <&pinctrl_usba_vbus>;
			status = "okay";
		};

		usb1: ohci@400000 {
			num-ports = <3>;
			atmel,vbus-gpio = <0 /* &pioA PIN_PB9 GPIO_ACTIVE_HIGH */
					   &pioA PIN_PB10 GPIO_ACTIVE_HIGH
					   0
					  >;
			pinctrl-names = "default";
			pinctrl-0 = <&pinctrl_usb_default>;
			status = "okay";
		};

		usb2: ehci@500000 {
			status = "okay";
		};

		sdmmc0: sdio-host@a0000000 {
			bus-width = <8>;
			pinctrl-names = "default";
			pinctrl-0 = <&pinctrl_sdmmc0_default>;
			non-removable;
			mmc-ddr-1_8v;
			status = "okay";
		};

		sdmmc1: sdio-host@b0000000 {
			bus-width = <4>;
			pinctrl-names = "default";
			pinctrl-0 = <&pinctrl_sdmmc1_default>;
			status = "okay"; /* conflict with qspi0 */
			vqmmc-supply = <&vdd_3v3_reg>;
			vmmc-supply = <&vdd_3v3_reg>;
		};

		apb {
			spi0: spi@f8000000 {
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_spi0_default>;
				status = "okay";

				m25p80@0 {
					compatible = "atmel,at25df321a", "jedec,spi-nor";
					reg = <0>;
					spi-max-frequency = <50000000>;
				};

				/* FIXME: max frequency needs to be verified:
				 * datasheet indicates 22.5MHZ at 1.8V
				 */
				/* FIXME: I think the IRQ is active low, but
				 * verify that it's not edge triggered
				 */
				tpm@2 {
					compatible = "infineon,slb9670";
					spi-max-frequency = <10000000>;
					reg = <2>;

					pinctrl-names = "default";
					pinctrl-0 = <&pinctrl_tpm_reset &pinctrl_tpm_irq>;
					interrupt-parent = <&pioA>;
					interrupts = <PIN_PD1 IRQ_TYPE_LEVEL_LOW>;
				};
			};

			spi1: spi@fc000000 {
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_spi1_default>;
				status = "okay";

				/* FIXME: add device corresponding to fiscal
				 * memory; currently, no driver exists for
				 * this thing and its functionality is
				 * currently still only very loosely defined
				 */
				/* For now, we put an spidev here so that we
				 * can talk to the MCU from userspace.
				 */
				spidev@0 {
					compatible = "retailinnovation,fismem";
					/* FIXME: what frequency?? */
					spi-max-frequency = <500000>;
					spi-word-delay-us = <24>;
					reg = <0>;
				};

				/* @1 we have an SPI-flash, but currently
				 * not known which model... put an spidev
				 * here for now so that we can poke it from
				 * userspace.
				 */
				fismem@1 {
					compatible = "spansion,s25fl512s", "jedec,spi-nor";
					spi-max-frequency = <50000000>;
					reg = <1>;
					m25p,fast-read;
				};
			};

			macb0: ethernet@f8008000 {
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_macb0_default &pinctrl_macb0_phy_irq>;
				phy-mode = "rmii";
				status = "okay";

				ethernet-phy@1 {
					reg = <0x1>;
					interrupt-parent = <&pioA>;
					interrupts = <PIN_PC9 IRQ_TYPE_LEVEL_LOW>;
				};
			};

			tcb0: timer@f800c000 {
				timer0: timer@0 {
					compatible = "atmel,tcb-timer";
					reg = <0>;
				};

				timer1: timer@1 {
					compatible = "atmel,tcb-timer";
					reg = <1>;
				};
			};

			uart0: serial@f801c000 {
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_uart0_default>;
				atmel,use-dma-rx;
				atmel,use-dma-tx;
				cts-gpios = <&pioA PIN_PC16 GPIO_ACTIVE_LOW>;
				rts-gpios = <&pioA PIN_PC17 GPIO_ACTIVE_LOW>;
				dsr-gpios = <&pioA PIN_PC18 GPIO_ACTIVE_LOW>;
				dtr-gpios = <&pioA PIN_PC19 GPIO_ACTIVE_LOW>;
				status = "okay";
			};

			uart1: serial@f8020000 {
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_uart1_default>;
				atmel,use-dma-rx;
				atmel,use-dma-tx;
				cts-gpios = <&pioA PIN_PC20 GPIO_ACTIVE_LOW>;
				rts-gpios = <&pioA PIN_PC21 GPIO_ACTIVE_LOW>;
				dsr-gpios = <&pioA PIN_PC22 GPIO_ACTIVE_LOW>;
				dtr-gpios = <&pioA PIN_PC23 GPIO_ACTIVE_LOW>;
				status = "okay";
			};

			uart2: serial@f8024000 {
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_uart2_default>;
				atmel,use-dma-rx;
				atmel,use-dma-tx;
				cts-gpios = <&pioA PIN_PC24 GPIO_ACTIVE_LOW>;
				rts-gpios = <&pioA PIN_PC25 GPIO_ACTIVE_LOW>;
				dsr-gpios = <&pioA PIN_PC26 GPIO_ACTIVE_LOW>;
				dtr-gpios = <&pioA PIN_PC27 GPIO_ACTIVE_LOW>;
				status = "okay";
			};

			i2c0: i2c@f8028000 {
				dmas = <0>, <0>;
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_i2c0_default>;
				i2c-sda-hold-time-ns = <350>;
				status = "okay";

				pmic@5b {
					compatible = "active-semi,act8945a";
					reg = <0x5b>;
					active-semi,vsel-high;
					status = "okay";

					regulators {
						vdd_1v35_reg: REG_DCDC1 {
							regulator-name = "VDD_1V35";
							regulator-min-microvolt = <1350000>;
							regulator-max-microvolt = <1350000>;
							regulator-always-on;
						};

						vdd_1v2_reg: REG_DCDC2 {
							regulator-name = "VDD_1V2";
							regulator-min-microvolt = <1100000>;
							regulator-max-microvolt = <1300000>;
							regulator-always-on;
						};

						vdd_3v3_reg: REG_DCDC3 {
							regulator-name = "VDD_3V3";
							regulator-min-microvolt = <3300000>;
							regulator-max-microvolt = <3300000>;
							regulator-always-on;
						};

						vdd_fuse_reg: REG_LDO1 {
							regulator-name = "VDD_FUSE";
							regulator-min-microvolt = <2500000>;
							regulator-max-microvolt = <2500000>;
							regulator-always-on;
						};

						vdd_3v3_lp_reg: REG_LDO2 {
							regulator-name = "VDD_3V3_LP";
							regulator-min-microvolt = <3300000>;
							regulator-max-microvolt = <3300000>;
							regulator-always-on;
						};

						vdd_led_reg: REG_LDO3 {
							regulator-name = "VDD_LED";
							regulator-min-microvolt = <3300000>;
							regulator-max-microvolt = <3300000>;
							regulator-always-on;
						};

						vdd_sdhc_1v8_reg: REG_LDO4 {
							regulator-name = "VDD_SDHC_1V8";
							regulator-min-microvolt = <1800000>;
							regulator-max-microvolt = <1800000>;
							regulator-always-on;
						};
					};

					charger {
						compatible = "active-semi,act8945a-charger";
						pinctrl-names = "default";
						pinctrl-0 = <&pinctrl_charger_chglev &pinctrl_charger_lbo &pinctrl_charger_irq>;
						interrupt-parent = <&pioA>;
						interrupts = <PIN_PB13 IRQ_TYPE_EDGE_RISING>;

						active-semi,chglev-gpios = <&pioA PIN_PA12 GPIO_ACTIVE_HIGH>;
						active-semi,lbo-gpios = <&pioA PIN_PC8 GPIO_ACTIVE_LOW>;
						active-semi,input-voltage-threshold-microvolt = <6600>;
						active-semi,precondition-timeout = <40>;
						active-semi,total-timeout = <3>;
						status = "okay";
					};
				};
			};

			shdwc@f8048010 {
				atmel,shdwc-debouncer = <976>;
				atmel,wakeup-rtc-timer;

				input@0 {
					reg = <0>;
					atmel,wakeup-type = "low";
				};
			};

			watchdog@f8048040 {
				status = "okay";
			};

			/* This should probably be disabled for
			 * production builds.
			 */
			uart4: serial@fc00c000 {
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_uart4_default>;
				atmel,use-dma-rx;
				atmel,use-dma-tx;
				status = "okay";
			};

			i2c1: i2c@fc028000 {
				dmas = <0>, <0>;
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_i2c1_default>;
				status = "okay";

				at24@54 {
					compatible = "atmel,24c02";
					reg = <0x54>;
					pagesize = <16>;
				};
			};

			/* The .dtsi that we inherate from assumes that there's
			 * a trigger GPIO... we don't have that so we drop
			 * the property
			 */
			adc: adc@fc030000 {
				vddana-supply = <&vdd_3v3_lp_reg>;
				vref-supply = <&vdd_3v3_lp_reg>;
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_adc_default>;
				/delete-property/atmel,trigger-edge-type;
				status = "okay";
			};

			pioA: pinctrl@fc038000 {
				n_gpios = <34>;
				gpio-reserved-ranges = <0 65>, <69 5>,
					<76 16>, <95 33>;
				gpio-line-names = "CPU_PRT", "CPU_DISP",
					"BAT_ACT", "BAT_LOW",
					"CPU_GPO0", "CPU_GPO1",
					"UARTOFF0", "UARTOFF1", "UARTOFF2";

				/*
				 * There is no real pinmux for ADC, if the pin
				 * is not requested by another peripheral then
				 * the muxing is done when channel is enabled.
				 * Requesting pins for ADC as GPIO is
				 * encouraged to prevent conflicts and to
				 * disable bias in order to be in the same
				 * state when the pin is not muxed to the adc.
				 */
				pinctrl_adc_default: adc_default {
					pinmux = <PIN_PD19__GPIO>;
					bias-disable;
				};

				pinctrl_charger_chglev: charger_chglev {
					pinmux = <PIN_PA12__GPIO>;
					bias-disable;
				};

				pinctrl_charger_irq: charger_irq {
					pinmux = <PIN_PB13__GPIO>;
					bias-disable;
				};

				pinctrl_charger_lbo: charger_lbo {
					pinmux = <PIN_PC8__GPIO>;
					bias-pull-up;
				};

				pinctrl_i2c0_default: i2c0_default {
					pinmux = <PIN_PD21__TWD0>,
						 <PIN_PD22__TWCK0>;
					bias-disable;
				};

				pinctrl_i2c1_default: i2c1_default {
					pinmux = <PIN_PD4__TWD1>,
						 <PIN_PD5__TWCK1>;
					bias-disable;
				};

				pinctrl_tpm_reset: tpm_reset {
					pinmux = <PIN_PD0__GPIO>;
					output-high;
				};

				pinctrl_tpm_irq: tpm_irq {
					pinmux = <PIN_PD1__GPIO>;
					bias-disable;
				};

				pinctrl_key_gpio_default: key_gpio_default {
					pinmux = <PIN_PB28__GPIO>,
						 <PIN_PB29__GPIO>,
						 <PIN_PB30__GPIO>,
						 <PIN_PB31__GPIO>;
					bias-disable;
				};

				pinctrl_mcu_led_on_default: mcu_led_on_default {
					pinmux = <PIN_PC6__GPIO>,
						 <PIN_PC10__GPIO>,
						 <PIN_PC11__GPIO>;
					output-high;
				};

				pinctrl_mcu_led_off_default: mcu_led_off_default {
					pinmux = <PIN_PC12__GPIO>,
						 <PIN_PC13__GPIO>,
						 <PIN_PC14__GPIO>,
						 <PIN_PC15__GPIO>;
					output-low;
				};

				pinctrl_mcu_gpio_in: mcu_gpio_in {
					pinmux = <PIN_PC3__GPIO>,
						 <PIN_PC4__GPIO>,
						 <PIN_PC5__GPIO>;
					bias-disable;
				};

				pinctrl_mcu_gpio_out: mcu_gpio_out {
					pinmux = <PIN_PC1__GPIO>,
						 <PIN_PC2__GPIO>;
					output-low;
				};

				pinctrl_led_gpio_default: led_gpio_default {
					pinmux = <PIN_PC0__GPIO>,
						 <PIN_PC5__GPIO>,
						 <PIN_PC6__GPIO>;
					bias-pull-up;
				};

				pinctrl_macb0_default: macb0_default {
					pinmux = <PIN_PB14__GTXCK>,
						 <PIN_PB15__GTXEN>,
						 <PIN_PB16__GRXDV>,
						 <PIN_PB17__GRXER>,
						 <PIN_PB18__GRX0>,
						 <PIN_PB19__GRX1>,
						 <PIN_PB20__GTX0>,
						 <PIN_PB21__GTX1>,
						 <PIN_PB22__GMDC>,
						 <PIN_PB23__GMDIO>;
					bias-disable;
				};

				pinctrl_macb0_phy_irq: macb0_phy_irq {
					pinmux = <PIN_PC9__GPIO>;
					bias-disable;
				};

				pinctrl_sdmmc0_default: sdmmc0_default {
					cmd_data {
						pinmux = <PIN_PA1__SDMMC0_CMD>,
							 <PIN_PA2__SDMMC0_DAT0>,
							 <PIN_PA3__SDMMC0_DAT1>,
							 <PIN_PA4__SDMMC0_DAT2>,
							 <PIN_PA5__SDMMC0_DAT3>,
							 <PIN_PA6__SDMMC0_DAT4>,
							 <PIN_PA7__SDMMC0_DAT5>,
							 <PIN_PA8__SDMMC0_DAT6>,
							 <PIN_PA9__SDMMC0_DAT7>;
						bias-disable;
					};

					ck_cd_rstn_vddsel {
						pinmux = <PIN_PA0__SDMMC0_CK>,
							 <PIN_PA10__SDMMC0_RSTN>,
							 <PIN_PA11__SDMMC0_VDDSEL>,
							 <PIN_PA13__SDMMC0_CD>;
						bias-disable;
					};
				};

				pinctrl_sdmmc1_default: sdmmc1_default {
					cmd_data {
						pinmux = <PIN_PA28__SDMMC1_CMD>,
							 <PIN_PA18__SDMMC1_DAT0>,
							 <PIN_PA19__SDMMC1_DAT1>,
							 <PIN_PA20__SDMMC1_DAT2>,
							 <PIN_PA21__SDMMC1_DAT3>;
						bias-disable;
					};

					conf-ck_cd {
						pinmux = <PIN_PA22__SDMMC1_CK>,
							 <PIN_PA30__SDMMC1_CD>;
						bias-disable;
					};
				};

				pinctrl_spi0_default: spi0_default {
					pinmux = <PIN_PA14__SPI0_SPCK>,
						 <PIN_PA15__SPI0_MOSI>,
						 <PIN_PA16__SPI0_MISO>,
						 <PIN_PA17__SPI0_NPCS0>,
						 <PIN_PA27__SPI0_NPCS2>;
					bias-disable;
				};

				pinctrl_spi1_default: spi1_default {
					pinmux = <PIN_PD25__SPI1_SPCK>,
						 <PIN_PD26__SPI1_MOSI>,
						 <PIN_PD27__SPI1_MISO>,
						 <PIN_PD28__SPI1_NPCS0>,
						 <PIN_PD29__SPI1_NPCS1>;
					bias-disable;
				};

				pinctrl_uart0_default: uart0_default {
					rx_tx {
						pinmux = <PIN_PB26__URXD0>,
							 <PIN_PB27__UTXD0>;
						bias-disable;
					};

					rtscts {
						pinmux = <PIN_PC16__GPIO>,
							 <PIN_PC17__GPIO>,
							 <PIN_PC18__GPIO>,
							 <PIN_PC19__GPIO>;
						bias-disable;
					};

					uartoff {
						pinmux = <PIN_PC28__GPIO>;
						output-high;
					};
				};

				pinctrl_uart1_default: uart1_default {
					rx_tx {
						pinmux = <PIN_PD2__URXD1>,
							 <PIN_PD3__UTXD1>;
						bias-disable;
					};

					rtscts {
						 pinmux = <PIN_PC20__GPIO>,
							 <PIN_PC21__GPIO>,
							 <PIN_PC22__GPIO>,
							 <PIN_PC23__GPIO>;
						bias-disable;
					};

					uartoff {
						pinmux = <PIN_PC29__GPIO>;
						output-high;
					};
				};

				pinctrl_uart2_default: uart2_default {
					rx_tx {
						pinmux = <PIN_PD23__URXD2>,
							 <PIN_PD24__UTXD2>;
						bias-disable;
					};

					rtscts {
						 pinmux = <PIN_PC24__GPIO>,
							 <PIN_PC25__GPIO>,
							 <PIN_PC26__GPIO>,
							 <PIN_PC27__GPIO>;
						bias-disable;
					};

					uartoff {
						pinmux = <PIN_PC30__GPIO>;
						output-high;
					};
				};

				pinctrl_uart4_default: uart4_default {
					pinmux = <PIN_PB3__URXD4>,
						 <PIN_PB4__UTXD4>;
					bias-disable;
				};

				pinctrl_usb_default: usb_default {
					pinmux = <PIN_PB10__GPIO>;
					bias-disable;
				};

				pinctrl_usba_vbus: usba_vbus {
					pinmux = <PIN_PA31__GPIO>;
					bias-disable;
				};
			};
		};
	};

	gpio_keys {
		compatible = "gpio-keys";

		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_key_gpio_default>;

		/* FIXME: decide on key codes */
		/* FIXME: should thes be set as 'wakeup-source'? */
		bp1 {
			label = "PB_USER1";
			gpios = <&pioA PIN_PB28 GPIO_ACTIVE_LOW>;
			linux,code = <0x104>;
		};
		bp2 {
			label = "PB_USER2";
			gpios = <&pioA PIN_PB29 GPIO_ACTIVE_LOW>;
			linux,code = <0x105>;
		};
		bp3 {
			label = "PB_USER3";
			gpios = <&pioA PIN_PB30 GPIO_ACTIVE_LOW>;
			linux,code = <0x106>;
		};
		bp4 {
			label = "PB_USER4";
			gpios = <&pioA PIN_PB31 GPIO_ACTIVE_LOW>;
			linux,code = <0x107>;
		};
	};

	leds {
		compatible = "gpio-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_led_gpio_default>;
		status = "okay"; /* conflict with pwm0 */

		/* Strobe from CPU to battery/power controller */
		stat {
			label = "stat";
			gpios = <&pioA PIN_PC0 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "heartbeat";
		};

		led2 {
			label = "led2";
			gpios = <&pioA PIN_PC5 GPIO_ACTIVE_LOW>;
		};

		led3 {
			label = "led3";
			gpios = <&pioA PIN_PC6 GPIO_ACTIVE_LOW>;
		};
	};
};
